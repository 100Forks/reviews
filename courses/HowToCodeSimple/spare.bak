;; about the language level of this file in a form that our tools can easily process.
#reader(lib "htdp-beginner-reader.ss" "lang")((modname spare) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
(define (sink lob)
  (cond [(empty? lob) empty]
        [(empty? (rest lob)) lob]
        [else ; (rest lob) is non-empty
         (cond [(string=? "bubble" (first lob))
                (cons "bubble" (sink (rest lob)))];(helper (first lob) (sink (rest lob)))
               [else ; (first lob) is "solid"
                (cond [(string=? "solid" (first (sink (rest lob))))
                       lob]
                      [else ;(first (sink (rest lob))) is "bubble"
                       (cons "bubble" (cons "solid" (rest (sink (rest lob)))))])])]))